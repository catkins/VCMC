{
  "configurations": [
    {
      "name": "Teensy",
      "includePath": [
        "${workspaceFolder}/**",
        "/Applications/Teensyduino.app/Contents/Java/hardware/arduino/avr/**",
        "${env:HOME}/Documents/Arduino/Libraries/**",
        "/Applications/Teensyduino.app/Contents/Java/libraries/**",
        "/Applications/Teensyduino.app/Contents/Java/hardware/tools/**",
        "/Applications/Teensyduino.app/Contents/Java/hardware/teensy/avr/**"
      ],
      "forcedInclude": [
        "/Applications/Teensyduino.app/Contents/Java/hardware/teensy/avr/cores/teensy3/Arduino.h",
        "/Applications/Teensyduino.app/Contents/Java/hardware/arduino/avr/cores/arduino/Arduino.h"
      ],
      "defines": [
        "__MK20DX256__",
        "TEENSYDUINO=153",
        "F_CPU=96000000",
        "USB_MIDI_SERIAL",
        "LAYOUT_US_ENGLISH",
        "CVTHING=1",
        "ARDUINO"
      ],
      "cStandard": "c11",
      "cppStandard": "c++11",
      "intelliSenseMode": "macos-gcc-arm"
    }
  ],
  "version": 4
}
